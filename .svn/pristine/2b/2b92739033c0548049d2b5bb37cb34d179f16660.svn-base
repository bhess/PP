package simulation.data;

import java.util.GregorianCalendar;


/**
 * @author bhess
 * 
 * Class to generate a sample configuration
 * 
 * -> Mainly for debugging purposes
 *
 */
public class ConfigurationSourceTestdata implements ConfigurationSource {

	/* (non-Javadoc)
	 * @see simulation.data.ConfigurationSource#getConfiguration(java.lang.String)
	 */
	public Configuration getConfiguration() {
		// TODO Auto-generated method stub
		Configuration config = new Configuration();
		config.startDate = GregorianCalendar.getInstance();
		config.runningTime = 24*60*60;
		
		
		return config;
	}

}
