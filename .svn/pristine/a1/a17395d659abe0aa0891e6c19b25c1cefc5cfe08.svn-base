package gates.unary;

import circuit.CircuitElement;

public class Not extends CircuitElement {
	/**
	 * @param inWireCnt
	 * @param outElementsCnt
	 */
	public Not(int outElementsCnt) {
		super(1, outElementsCnt);
	}

	public boolean evaluate(boolean[] in) {
		return !in[0];
	}

	@Override
	public int getArity() {
		// TODO Auto-generated method stub
		return 1;
	}
}
